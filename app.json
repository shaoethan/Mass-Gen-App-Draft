{
  "expo": {
    "runtimeVersion": "1.0.0",
    "name": "ActiPain Tracker",
    "slug": "actipain-tracker",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/redlogo.png",
    "scheme": "mghapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "buildNumber":"101",
      "supportsTablet": true,
      "bundleIdentifier": "com.ninjaprime.MGHapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ninjaprime.MGHapp",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
          "useFrameworks": "static",
          "modular_headers": true,
          "deploymentTarget": "15.1"
        }
        }
      ],
      
      "expo-audio",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/fa8ae35f-7205-4355-8699-5ed4dc46c15e",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "17f5cc34-321f-443b-ab63-262f168bb4c0"
      }
    },
    "owner": "ninjaprime"
  }
}
