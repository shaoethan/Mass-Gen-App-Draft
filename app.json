{
  "expo": {
    "name": "ActiPain Tracker",
    "slug": "actipain-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/redlogo.jpg",
    "scheme": "mghapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ninjaprime.MGHapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ninjaprime.MGHapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fa8ae35f-7205-4355-8699-5ed4dc46c15e"
      }
    },
    "owner": "ninjaprime",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/fa8ae35f-7205-4355-8699-5ed4dc46c15e"
    }
  }
}
